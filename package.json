{
    "name": "my-addon",
    "version": "1.0.0",
    "description": "Adobe Creative Cloud Web Add-on with Text Warp functionality.",
    "type": "module",
    "scripts": {
        "build": "babel src/ui --out-dir src/ui --extensions .jsx",
        "dev": "npm run build && ccweb-add-on-scripts start",
        "clean": "ccweb-add-on-scripts clean",
        "start": "ccweb-add-on-scripts start",
        "package": "ccweb-add-on-scripts package"
    },
    "keywords": [
        "Adobe",
        "Creative Cloud Web",
        "Add-on",
        "panel"
    ],
    "dependencies": {
        "opentype.js": "^1.3.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@adobe/ccweb-add-on-scripts": "^3.0.0",
        "@babel/cli": "^7.27.2",
        "@babel/core": "^7.27.4",
        "@babel/preset-react": "^7.27.1",
        "@types/adobe__ccweb-add-on-sdk": "^1.3.0"
    }
}
